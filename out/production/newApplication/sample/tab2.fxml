            <ImageView fitHeight="199.0" fitWidth="256.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../Images/clipart21451.png" />
               </image>
            </ImageView>
            <Label text="ID" textFill="WHITE">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <TextField fx:id="idPrpprietaire" />
            <Label layoutX="30.0" layoutY="224.0" text="Nom" textFill="WHITE">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <TextField fx:id="nomPropietaire" layoutX="30.0" layoutY="266.0" />
            <Label layoutX="30.0" layoutY="306.0" text="Prenom" textFill="WHITE">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <TextField fx:id="prenomProprietaire" layoutX="30.0" layoutY="348.0" />
            <Label layoutX="30.0" layoutY="388.0" text="Adress" textFill="WHITE">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <TextField fx:id="adressProprietaire" layoutX="30.0" layoutY="430.0" />
            <Label layoutX="30.0" layoutY="470.0" text="Contact" textFill="WHITE">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <TextField fx:id="contactProprietaire" layoutX="30.0" layoutY="512.0" />
            <Button mnemonicParsing="false" onAction="#AjouterProprietaire" style="-fx-background-color: #7BCCB5;" text="Ajouter">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
         </children>
         <padding>
            <Insets left="20.0" right="20.0" />
         </padding>
      </VBox>
      <TableView fx:id="tableProprietaire" layoutX="321.0" layoutY="188.0" prefHeight="474.0" prefWidth="656.0">

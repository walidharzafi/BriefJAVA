          <TableColumn fx:id="tbIdProp" prefWidth="75.0" text="ID" />
          <TableColumn fx:id="tbNomProprietaire" prefWidth="151.0" text="Nom" />
            <TableColumn fx:id="tbPrenomProp" prefWidth="154.0" text="Prenom" />
            <TableColumn fx:id="tbAdressProp" prefWidth="159.0" text="Adress" />
            <TableColumn fx:id="tbContactProp" prefWidth="116.0" text="Contact" />
        </columns>
      </TableView>
      <Button layoutX="884.0" layoutY="688.0" mnemonicParsing="false" onAction="#supprimerProprietaire" text="Supprimer">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
